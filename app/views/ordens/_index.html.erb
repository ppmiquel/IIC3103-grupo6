<div class="padding-md">
          <ul class="breadcrumb">
            <li><span class="primary-font"><i class="icon-home"></i></span><a>Inicio</a></li>
            <li>Órdenes de compra</li>
          </ul>
          <table class="table table-striped" id="dataTable">
            <thead>
            <tr>
              <th>Id OC</th>
              <th>Canal</th>
              <th>Cliente</th>
              <th>Sku</th>
              <th>Cantidad</th>
              <th>Precio</th>
              <th>Fecha entrega</th>
              <th>Id Transacción</th>
              <th>Id Factura</th>
              <th>Estado</th>
              <th colspan="3"></th>
            </tr>
            </thead>

            <tbody>
            <% @ordens.each do |orden| %>
                <tr>
                  <td><%= orden.idoc %></td>
                  <td><%= orden.canal %></td>
                  <td><%= orden.cliente %></td>
                  <td><%= orden.sku %></td>
                  <td><%= orden.cantidad %></td>
                  <td><%= orden.precio %></td>
                  <td><%= orden.fecha_entrega %></td>
                  <td><%= orden.idtrx %></td>
                  <td><%= orden.idfact %></td>
                  <td><%= orden.estado %></td>
                </tr>
            <% end %>
            </tbody>
          </table>
        </div><!-- ./padding-md -->
